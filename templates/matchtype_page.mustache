<div id='vocab-list'>
  {{> mod_wordcards/vocablist_header}}
  <div id="vocab-list-inner">{{> mod_wordcards/loading}}</div>
</div>

<div id='gameboard'>

  <div class="mod_wordcards_activity_header">
    <h3>{{#str}}title_matchtype, mod_wordcards{{/str}}</h3>
    <div class="mod_wordcards_instructions">{{#str}}instructions_matchtype, mod_wordcards{{/str}}</div>
  </div>

  <div id='counter'>
    <div id='time-counter'></div>
  </div>

  {{> mod_wordcards/progress_dots}}

  <div id='question'></div>
  <div id='submitted'></div>
  <div id='input'></div>

</div>

<div id='results'>
  <div id='results-inner'>{{> mod_wordcards/loading}}</div>
</div>